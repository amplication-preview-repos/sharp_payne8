datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Email {
  body             String?
  createdAt        DateTime  @default(now())
  id               String    @id @default(cuid())
  receivedDateTime DateTime?
  subject          String?
  updatedAt        DateTime  @updatedAt
}

model Sender {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
}
